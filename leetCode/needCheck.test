def calDelete(big, nums):
    if len(nums) == 0 :
        return 0
    d = 0
    tag = nums[0]
    for i in nums[1:]:
        if i <= tag:
            d += 1
        elif i > big:
            return d
    return d

def search_sum(tag,infos):
    sum = 1
    if len(infos) == 0 :
        return sum
    if tag <= infos[0]:
        if max(infos) > infos[0]:
            maxF =  max(infos)
            m = 0
            while m < len(infos):
                if infos[m] == maxF:
                    return sum + 1
                else:
                    sum += 1
                m += 1
        return sum + 1
    else:
        sum += search_sum(tag,infos[0+1:])
        sum -= calDelete(tag, infos)

    return sum


n = 10
